{
  "name": "jquery-datasource",
  "version": "1.0.0",
  "description": "A little jQuery plugin to manage server rendered data with pagination, sorting and filtering.",
  "main": "src/jquery-datasource.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack --progress --mode development --config webpack.config.js",
    "watch": "npm run dev -- --watch",
    "production": "cross-env NODE_ENV=production webpack -p --progress --mode production --config webpack.config.js"
  },
  "keywords": [
    "jquery",
    "datasource"
  ],
  "author": "manrix",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "deparam": "^1.0.5",
    "jquery": "^3.5.0",
    "lodash.debounce": "^4.0.8"
  }
}
